# Neo4j Environment Variables
NEO4J_IMAGE=neo4j
NEO4J_IMAGE_TAG=latest
NEO4J_WEB_PORT=7474
NEO4J_BOLT_PORT=7687
NEO4J_AUTH=neo4j/your_password

# Redis Environment Variables
REDIS_IMAGE=redis
REDIS_IMAGE_TAG=latest
REDIS_HEALTHCHECK_INTERVAL=5s
REDIS_HEALTHCHECK_TIMEOUT=1s
REDIS_HEALTHCHECK_RETRIES=5
REDIS_HOST_PORT=6379
REDIS_CONTAINER_PORT=6379

# PostgreSQL Environment Variables
POSTGRES_IMAGE=postgres
POSTGRES_IMAGE_TAG=latest
POSTGRES_HOST_PORT=5433
POSTGRES_CONTAINER_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HEALTHCHECK_START_PERIOD=10s
POSTGRES_HEALTHCHECK_TIMEOUT=1s
POSTGRES_HEALTHCHECK_RETRIES=5
POSTGRES_HEALTHCHECK_INTERVAL=5s

# Optional API Configuration (if needed)
# LANGGRAPH_API_IMAGE=my-image
# LANGGRAPH_API_HOST_PORT=8123
# LANGGRAPH_API_CONTAINER_PORT=8000
# OPENAI_API_KEY=xxx
# LANGSMITH_API_KEY=xxx
# POSTGRES_URI=postgres://postgres:postgres@langgraph-postgres:5432/postgres?sslmode=disable

# Metrics Configuration (if needed)
# NEO4J_METRICS_PORT=9600
